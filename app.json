{
  "expo": {
    "name": "Pixelito",
    "slug": "pixelito",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",  // Corregido: a√±adido "splash" al nombre del archivo
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jardino.pixelito"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jardino.pixelito",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c32ce86a-95dc-419e-9ca9-d84005fa7424"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir a Pixelito acceder a tus fotos.",
          "savePhotosPermission": "Permitir a Pixelito guardar fotos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "owner": "jardino"
  }
}